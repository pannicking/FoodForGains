<head>
    <link rel="stylesheet" type="text/css" href="styles.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body background="https://wallpapers.com/images/hd/restaurant-background-ua09rq3u85yphzt4.jpg">
    <%- include ('partials/navbar.ejs') %>
        <section id="feedback" class="container py-5">

            <div class="container mt-3 ">
                <h2 style="color:white; font-family: 'Times New Roman', serif;">Give Us Your Feedback</h2>
                <form action="/submitfeedback" method="post">
                    <div class="mb-3 mt-3">
                        <label for="name" style="color: #ffffff; font-family: 'Times New Roman', serif;">Name:</label>
                        <input type="name" class="form-control" id="name" placeholder="Enter Name" name="name">
                    </div>
                    <div class="mb-3 mt-3">
                        <label for="email" style="color: #ffffff; font-family: 'Times New Roman', serif;">Email:</label>
                        <input type="email" class="form-control" id="email" placeholder="Enter Email" name="email">
                    </div>

   
                    

                    <div class="mb-3 mt-3">
                        <label for="feedback" style="color: #ffffff; font-family: 'Times New Roman', serif;">Feedback:</label>
                        <input type="feedback" class="form-control" id="feedback" placeholder="Enter your feedback" name="feedback">
                    </div>

                     




                    <button type="submit" class="btn btn-primary" style="background-color: #301e0d; color: #ffffff;">Submit</button>
                </form>
            </div>
        </section>
</body>